    <select id="queryEqpIdStr" parameterType="map" resultType="java.lang.String">
        select EQ_LIST from DWR_OEE_MASTER
        WHERE factory = #{factory} and BIG_EQP_TYPE = #{groupName}
    </select>
    <select id="queryBigEqpTypes" parameterType="map" resultType="java.lang.String">
        SELECT BIG_EQP_TYPE from DWR_OEE_MASTER WHERE factory = #{factory}
    </select>
    <select id="queryActivationByEQPIdListAndFactory" parameterType="map" resultType="com.xm.service.apiimpl.pc.cim.oee.dto.ActivationDate$StatusDateList">
        SELECT
        EQP_STATUS as status,
        sum(NVL(STATUS_DURATION,0)) as statusDuration
        FROM
        DWR_EQP_OEE_FIDS OEE
        WHERE
        OEE.FACTORY IN
        <foreach collection="factoryList" item="factory" open="(" separator="," close=")">
            #{factory}
        </foreach>
        AND OEE.EQP_ID in
        <foreach collection="eqpIdList" item="eqpId" open="(" separator="," close=")">
            #{eqpId}
        </foreach>
        <![CDATA[
          and PERIODDATE >= to_date('${beginDateStr}','yyyy-mm-dd hh24:mi:ss')
          and PERIODDATE <= to_date('${endDateStr}','yyyy-mm-dd hh24:mi:ss')
          ]]>
        GROUP BY EQP_STATUS
    </select>

