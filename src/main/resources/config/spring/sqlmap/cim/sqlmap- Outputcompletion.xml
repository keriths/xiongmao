<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xm.service.dao.cim.OutputcompletionDAO" >
    <resultMap id="BaseResultMap" type="com.xm.service.apiimpl.pc.cim.Outputcompletion.OutputCompletionData">
        <result column="PLAN_OUTPUT_PNL_QTY" property="plan" jdbcType="DOUBLE"></result>
        <result column="ACTUAL_OUTPUT_PNL_QTY" property="actual" jdbcType="DOUBLE"></result>
        <!--<result column="" property="completionRate" jdbcType=""></result>-->
        <result column="PERIODDATE" property="date" jdbcType="DATE"></result>
    </resultMap>
    
    <select id="OutputCompletionRate" resultMap="BaseResultMap" parameterType="java.lang.String">
      select PLAN_OUTPUT_PNL_QTY,ACTUAL_OUTPUT_PNL_QTY,PERIODDATE from DWS_PRODUCT_OUTPUT_FIDS WHERE FACTORY=#{0,jdbcType=VARCHAR}
    </select>
</mapper>