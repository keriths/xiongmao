<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xm.service.dao.fmcs.CDADataDAO" >

    <select id="queryCdaData" resultType="com.xm.service.apiimpl.pc.fmcs.cda.dto.CdaData" parameterType="Map">
        select
            KEY as key,
            VAL as val,
            KEYDESC as keyDesc,
            DATATYPE as dataType,
            DEMODATA as demoData,
            DATADATE as dataDate,
            UNIT as unit
        from CDADATA
    </select>

    <!--根据key查询-->
    <select id="queryStatusByKey" parameterType="map" resultType="com.xm.service.apiimpl.pc.fmcs.cda.dto.CdaData">
        select
            KEY as key,
            VAL as val,
            KEYDESC as keyDesc,
            DATATYPE as dataType,
            DEMODATA as demoData,
            DATADATE as dataDate
        from CDADATA
        WHERE
            KEY=#{key}
    </select>

    <update id="updateStatusData" parameterType="com.xm.service.apiimpl.pc.fmcs.cda.dto.CdaData">
        UPDATE CDADATA
        SET KEY=#{key},
            VAL=#{val},
            DATADATE=#{dataDate}
        WHERE
          PCWDATA.KEY=#{key}
    </update>

    <insert id="insertStatusData" parameterType="com.xm.service.apiimpl.pc.fmcs.cda.dto.CdaData">
        INSERT INTO CDADATA(KEY,VAL,DATADATE) VALUES (#{key},#{val},#{dataDate})
    </insert>

</mapper>