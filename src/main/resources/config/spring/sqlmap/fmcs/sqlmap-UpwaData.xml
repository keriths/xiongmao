<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xm.service.dao.fmcs.UpwaDataDAO" >

    <select id="queryUpwaDataList" resultType="com.xm.service.apiimpl.pc.fmcs.upw.dto.UpwaData" parameterType="Map">
        SELECT
            KEY AS key,
            VALUE AS value,
            KEYDESC AS keyDesc,
            DATADATE AS dataDate,
            DATATYPE AS dataType,
            DEMODATA AS demoData,
            UNIT as unit
        FROM UPWADATA
    </select>


    <!--根据key查询-->
    <select id="queryStatusByKey" parameterType="map" resultType="com.xm.service.apiimpl.pc.fmcs.upw.dto.UpwaData">
        select
            KEY as key,
            VAL as value,
            KEYDESC as keyDesc,
            DATATYPE as dataType,
            DEMODATA as demoData,
            DATADATE as dataDate
        from UPWADATA
        WHERE
            KEY=#{key}
    </select>

    <update id="updateStatusData" parameterType="com.xm.service.apiimpl.pc.fmcs.upw.dto.UpwaData">
        UPDATE UPWADATA
        SET KEY=#{key},
            VAL=#{value},
            DATADATE=#{dataDate}
        WHERE
            KEY=#{key}
    </update>

    <insert id="insertStatusData" parameterType="com.xm.service.apiimpl.pc.fmcs.upw.dto.UpwaData">
        INSERT INTO UPWADATA(KEY,VAL,DATADATE) VALUES (#{key},#{value},#{dataDate})
    </insert>

</mapper>