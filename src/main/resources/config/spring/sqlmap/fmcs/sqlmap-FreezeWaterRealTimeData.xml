<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xm.service.dao.fmcs.FreezeWaterRealTimeDataDAO" >

    <select id="freezeWaterRealTimeData" resultType="com.xm.service.apiimpl.pc.fmcs.water.dto.WaterRealTimeDate" parameterType="Map">
        select
        Speed as speed,
        DataDate as dataDate
        from FreezeWaterRealTimeData
        WHERE
        <![CDATA[
               DataDate >= #{beginDate}
              and DataDate <= #{endDate}
        ]]>
    </select>

</mapper>