<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xm.service.dao.cim.OutputcompletionDAO" >
    <resultMap id="BaseResultMap" type="com.xm.service.apiimpl.pc.cim.Outputcompletion.OutputCompletionData">
        <result column="FACTORY" property="factory" jdbcType="VARCHAR"></result>
        <result column="PLAN_OUTPUT_PNL_QTY" property="plan" jdbcType="DOUBLE"></result>
        <result column="ACTUAL_OUTPUT_PNL_QTY" property="actual" jdbcType="DOUBLE"></result>
        <!--<result column="" property="completionRate" jdbcType=""></result>-->
        <result column="PERIODDATE" property="dateTime" jdbcType="VARCHAR"></result>
    </resultMap>
    
    <select id="OutputCompletionRate" resultMap="BaseResultMap" parameterType="Map">
        select
        <if test="dateType=='m'.toString()">/*按月*/
          TO_CHAR(PERIODDATE,'YYYY-MM') as dateTime,
        </if>
        <if test="dateType=='w'.toString()">/*按周*/
          TO_CHAR(PERIODDATE,'YYYY-MM') as dateTime,
        </if>
        <if test="dateType=='d'.toString()">/*按天*/
        TO_CHAR(PERIODDATE,'dd') as dateTime,
        </if>
        sum(PLAN_OUTPUT_PNL_QTY) as plan,sum(ACTUAL_OUTPUT_PNL_QTY) as actual,FACTORY from DWS_PRODUCT_OUTPUT_FIDS
        <![CDATA[
          WHERE PERIODDATE >= #{beginDate}
          and PERIODDATE <= #{endDate}
         ]]>
        <if test="product!=null and product!=''">
            AND PRODUCTTYPE=#{product,jdbcType=VARCHAR}
        </if>
        GROUP BY
            <if test="dateType=='m'.toString()">/*按月*/
            TO_CHAR(PERIODDATE,'YYYY-MM'),
            </if>
            <if test="dateType=='w'.toString()">
                TO_CHAR(PERIODDATE,'YYYY-MM'),
            </if>
            <if test="dateType=='d'.toString()">
                TO_CHAR(PERIODDATE,'dd'),
            </if>
        FACTORY
    </select>
</mapper>