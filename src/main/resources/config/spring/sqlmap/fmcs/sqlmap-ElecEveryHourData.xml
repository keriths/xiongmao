<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xm.service.dao.fmcs.ElecEveryHourDataDAO" >

    <select id="electricityDate" resultType="com.xm.service.apiimpl.pc.fmcs.electricity.dto.ElectricityDate" parameterType="Map">
        select
        PlaceType as placeType,
        DataDate as dataDate,
        TotalNum as totalNum
        from ElecEveryHourData
        WHERE
        <![CDATA[
               DataDate >= #{beginDate}
              and DataDate <= #{endDate}
        ]]>
    </select>

</mapper>